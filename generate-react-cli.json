{
  "usesTypeScript": true,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates": {
        "component": "grc/function/TemplateName.tsx"
      }
    },
    "ui": {
      "path": "src/components/ui",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates": {
        "component": "grc/function/TemplateName.tsx"
      }
    },
    "layout": {
      "path": "src/components/layout",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates": {
        "component": "grc/layout-function/TemplateName.tsx",
        "style": "grc/layout-function/TemplateName.module.css"
      }
    },
    "page": {
      "path": "src/pages",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates": {
        "component": "grc/nude-function/TemplateName.tsx",
        "story":"grc/nude-function/readme.md"
      }
    }
  }
}